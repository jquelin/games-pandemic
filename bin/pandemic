use 5.010;
use strict;
use warnings;

package pandemic;
{
  $pandemic::VERSION = '1.120510';
}
# ABSTRACT: pandemic board game in perl

use FindBin qw{ $Bin };
use lib "$Bin/../lib";

use Games::Pandemic;

Games::Pandemic->new;
Games::Pandemic->run;
exit;



